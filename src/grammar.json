{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mail",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_headers"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "body_separator"
                },
                {
                  "type": "SYMBOL",
                  "name": "body"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_headers": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_header"
          },
          {
            "type": "SYMBOL",
            "name": "newline"
          }
        ]
      }
    },
    "_header": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "header_email"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "header_subject"
          }
        },
        {
          "type": "SYMBOL",
          "name": "header_other"
        }
      ]
    },
    "header_email": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_field_email"
        },
        {
          "type": "SYMBOL",
          "name": "header_separator"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "atom"
              },
              {
                "type": "SYMBOL",
                "name": "quoted_string"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "email"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "header_other": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_field"
        },
        {
          "type": "SYMBOL",
          "name": "header_separator"
        },
        {
          "type": "SYMBOL",
          "name": "header_unstructured"
        }
      ]
    },
    "header_subject": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_field_subject"
        },
        {
          "type": "SYMBOL",
          "name": "header_separator"
        },
        {
          "type": "SYMBOL",
          "name": "header_unstructured"
        }
      ]
    },
    "header_separator": {
      "type": "STRING",
      "value": ":"
    },
    "header_field": {
      "type": "PATTERN",
      "value": "[^\\x00-\\x1f\\x7f\\s:]+"
    },
    "header_field_email": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "From"
        },
        {
          "type": "STRING",
          "value": "To"
        },
        {
          "type": "STRING",
          "value": "Cc"
        },
        {
          "type": "STRING",
          "value": "Bcc"
        },
        {
          "type": "STRING",
          "value": "Reply-To"
        }
      ]
    },
    "header_field_subject": {
      "type": "STRING",
      "value": "Subject"
    },
    "header_unstructured": {
      "type": "PATTERN",
      "value": ".*"
    },
    "atom": {
      "type": "PATTERN",
      "value": "[^()<>@,;:\\\\\".\\[\\]\\s\\x00-\\x1f\\x7f]+"
    },
    "quoted_string": {
      "type": "PATTERN",
      "value": "\"[^\"\\\\\\n]+\""
    },
    "email": {
      "type": "PATTERN",
      "value": "<[^<>]+>"
    },
    "body_separator": {
      "type": "SYMBOL",
      "name": "newline"
    },
    "body": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PREC",
            "value": 1,
            "content": {
              "type": "SYMBOL",
              "name": "quoted_line"
            }
          },
          {
            "type": "SYMBOL",
            "name": "body_line"
          },
          {
            "type": "SYMBOL",
            "name": "empty_line"
          }
        ]
      }
    },
    "quoted_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n]*"
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "body_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\r\\n]*"
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "empty_line": {
      "type": "SYMBOL",
      "name": "newline"
    },
    "newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": " "
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}