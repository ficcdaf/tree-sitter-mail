{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mail",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_headers"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "body_separator"
                },
                {
                  "type": "SYMBOL",
                  "name": "body"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_headers": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_header"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "_header": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "header_email"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "header_subject"
          }
        },
        {
          "type": "SYMBOL",
          "name": "header_other"
        }
      ]
    },
    "header_email": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_field_email"
        },
        {
          "type": "SYMBOL",
          "name": "header_separator"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "atom_block"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "email"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "header_other": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_field"
        },
        {
          "type": "SYMBOL",
          "name": "header_separator"
        },
        {
          "type": "SYMBOL",
          "name": "header_unstructured"
        }
      ]
    },
    "header_subject": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_field_subject"
        },
        {
          "type": "SYMBOL",
          "name": "header_separator"
        },
        {
          "type": "SYMBOL",
          "name": "subject"
        }
      ]
    },
    "header_separator": {
      "type": "STRING",
      "value": ":"
    },
    "header_field": {
      "type": "PATTERN",
      "value": "[^\\x00-\\x1f\\x7f\\s:]+"
    },
    "header_field_email": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "From"
        },
        {
          "type": "STRING",
          "value": "To"
        },
        {
          "type": "STRING",
          "value": "Cc"
        },
        {
          "type": "STRING",
          "value": "Bcc"
        },
        {
          "type": "STRING",
          "value": "Reply-To"
        }
      ]
    },
    "header_field_subject": {
      "type": "STRING",
      "value": "Subject"
    },
    "header_unstructured": {
      "type": "PATTERN",
      "value": ".*"
    },
    "subject": {
      "type": "PATTERN",
      "value": ".*"
    },
    "atom_block": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "atom"
          },
          {
            "type": "SYMBOL",
            "name": "quoted_string"
          }
        ]
      }
    },
    "atom": {
      "type": "PATTERN",
      "value": "[^()<>@,;:\\\\\".\\[\\]\\s\\x00-\\x1f\\x7f]+"
    },
    "quoted_string": {
      "type": "PATTERN",
      "value": "\"[^\"\\\\\\n]+\""
    },
    "email_delimiter": {
      "type": "CHOICE",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": ">"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "<"
          }
        }
      ]
    },
    "email_address": {
      "type": "PATTERN",
      "value": "(?:[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*|\"(?:[\\x01-\\x08\\x0b\\x0c\\x0e-\\x1f\\x21\\x23-\\x5b\\x5d-\\x7f]|\\\\[\\x01-\\x09\\x0b\\x0c\\x0e-\\x7f])*\")@(?:(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?|\\[(?:(?:(2(5[0-5]|[0-4][0-9])|1[0-9][0-9]|[1-9]?[0-9]))\\.){3}(?:(2(5[0-5]|[0-4][0-9])|1[0-9][0-9]|[1-9]?[0-9])|[a-z0-9-]*[a-z0-9]:(?:[\\x01-\\x08\\x0b\\x0c\\x0e-\\x1f\\x21-\\x5a\\x53-\\x7f]|\\\\[\\x01-\\x09\\x0b\\x0c\\x0e-\\x7f])+)\\])"
    },
    "email": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "email_delimiter"
        },
        {
          "type": "SYMBOL",
          "name": "email_address"
        },
        {
          "type": "SYMBOL",
          "name": "email_delimiter"
        }
      ]
    },
    "body_separator": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "body": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PREC",
            "value": 3,
            "content": {
              "type": "SYMBOL",
              "name": "_empty_line"
            }
          },
          {
            "type": "PREC",
            "value": 2,
            "content": {
              "type": "SYMBOL",
              "name": "quote_group"
            }
          },
          {
            "type": "PREC",
            "value": 1,
            "content": {
              "type": "SYMBOL",
              "name": "body_block"
            }
          }
        ]
      }
    },
    "quote_group": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "_quoted_line"
        }
      }
    },
    "quoted_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quote_marker"
        },
        {
          "type": "SYMBOL",
          "name": "quote_contents"
        }
      ]
    },
    "_quoted_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_block"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "quote_marker": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": ">"
      }
    },
    "quote_contents": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\r\\n]*"
      }
    },
    "body_block": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "_body_line"
        }
      }
    },
    "_body_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\r\\n>].*"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "_empty_line": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": " "
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}